{
  "guildName": "Floor-Gang",
  "appName": "$t(guildName) Modmail",
  "viewGithub": "View this projects Github repository!",
  "maxConcurrentRequests": 2,
  "urls": {
    "base": "/api",
    "discordBase": "https://discord.com/api",
    "oauth": "$t(urls.base)/oauth{{redirect}}",
    "logout": "$t(urls.base)/logout",
    "authenticate": "$t(urls.base)/self",
    "categories": "$t(urls.base)/categories",
    "categoriesOne": "$t(urls.base)/categories/{{category}}",
    "threads": "$t(urls.base)/categories/{{category}}/threads",
    "threadsOne": "$t(urls.base)/categories/{{category}}/threads/{{thread}}",
    "fetchThreadsByUser": "$t(urls.base)/categories/{{category}}/users/{{user}}/history",
    "fetchMember": "$t(urls.base)/categories/{{category}}/users/{{member}}",
    "fetchMembers": "$t(urls.base)/categories/{{category}}/members",
    "fetchDiscordMember": "$t(urls.discordBase)/users/@me",
    "fetchRole": "$t(urls.base)/categories/{{category}}/roles/{{role}}",
    "fetchChannel": "$t(urls.base)/categories/{{category}}/channels/{{channel}}"
  },
  "chips": {
    "internal": "Internal",
    "creator": "Creator",
    "deleted": "Removed",
    "active": "Active",
    "inactive": "Inactive",
    "public": "Shared Access",
    "private": "Admin Only Access"
  },
  "tooltips": {
    "category": {
      "status": "{{category}} is currently {{status}}",
      "public": "{{category}} is viewable by all mods",
      "private": "{{category}} is viewable by admins only"
    },
    "message": {
      "name": "Click to view <b>{{user}}</b>'s history"
    },
    "discord": {
      "emptyChannel": "Channel '{{id}}' could not be found",
      "foundChannel": "Channel ID: {{id}}",
      "emptyRole": "Role '{{id}}' could not be found",
      "foundRole": "Role ID: {{id}}",
      "emptyUser": "User '{{id}}' could not be found",
      "foundUser": "User ID: {{id}}"
    },
    "thread": {
      "active": "This thread is currently active",
      "inactive": "This thread is currently inactive",
      "locked": "This thread is Admin Only",
      "unlocked": "This thread is available to all mods"
    }
  },
  "drawer": {
    "tabs": {
      "categories": "Categories",
      "threads": "Threads"
    }
  },
  "dialogs": {
    "userLookup": {
      "title": "User History Lookup",
